from typing import Any, Callable, overload, Self
from arolla import arolla
from koladata.types import data_bag
from koladata.types import data_slice
from koladata.types import type_defs


class DataItem(data_slice.DataSlice):
    def __call__(self, *args: Any, return_type_as: Any = ..., **kwargs: Any) -> data_slice.DataSlice | arolla.Expr: ...
    def bind(self, *args: Any, return_type_as: Any = ..., **kwargs: Any) -> DataItem: ...

    def __float__(self) -> float: ...
    def __index__(self) -> int: ...
    def __int__(self) -> int: ...
    def __hash__(self) -> int: ...

    # Methods inherited from DataSlice that return DataItem for DataItem, but
    # don't return Self for all DataSlice sub-classes (e.g. ListItem, DictItem).
    # We aim to make this list exhaustive, but it's a best effort approach.
    def list_size(self) -> DataItem: ...
    def dict_size(self) -> DataItem: ...
    def get_size(self) -> DataItem: ...

    def get_itemid(self) -> DataItem: ...
    def get_present_count(self) -> DataItem: ...

    @overload
    def get_attr(self, attr_name: data_slice.DataSlice, /, default: Any = None) -> data_slice.DataSlice: ...
    @overload
    def get_attr(self, attr_name: str, /, default: Any = None) -> DataItem: ...
    @overload
    def has_attr(self, attr_name: str) -> DataItem: ...
    @overload
    def has_attr(self, attr_name: data_slice.DataSlice, /) -> data_slice.DataSlice: ...
    @overload
    def maybe(self, attr_name: str) -> DataItem: ...
    @overload
    def maybe(self, attr_name: data_slice.DataSlice, /) -> data_slice.DataSlice: ...
    @overload
    def with_attr(self, attr_name: str, value: Any, overwrite_schema: bool = False) -> Self: ...
    @overload
    def with_attr(self, attr_name: data_slice.DataSlice, value: Any, overwrite_schema: bool | data_slice.DataSlice = False) -> data_slice.DataSlice: ...

    @overload
    def __add__(self, other: DataItem | int | float | str) -> DataItem: ...
    @overload
    def __add__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __radd__(self, other: DataItem | int | float | str) -> DataItem: ...
    @overload
    def __radd__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __sub__(self, other: DataItem | int | float) -> DataItem: ...
    @overload
    def __sub__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __rsub__(self, other: DataItem | int | float) -> DataItem: ...
    @overload
