    @overload
    def __rpow__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __and__(self, other: DataItem | bool | str | int | float) -> DataItem: ...
    @overload
    def __and__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
