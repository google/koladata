    @overload
    def __rmul__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __truediv__(self, other: DataItem | int | float) -> DataItem: ...
    @overload
    def __truediv__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __rtruediv__(self, other: DataItem | int | float) -> DataItem: ...
    @overload
    def __rtruediv__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __floordiv__(self, other: DataItem | int | float) -> DataItem: ...
    @overload
    def __floordiv__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __rfloordiv__(self, other: DataItem | int | float) -> DataItem: ...
    @overload
    def __rfloordiv__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __mod__(self, other: DataItem | int | float) -> DataItem: ...
    @overload
    def __mod__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __rmod__(self, other: DataItem | int | float) -> DataItem: ...
