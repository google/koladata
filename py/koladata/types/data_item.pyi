    @overload
    def __le__(self, other: DataItem | int | float) -> DataItem: ...
    @overload
    def __le__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __lshift__(self, other: DataItem | int) -> DataItem: ...
    @overload
    def __lshift__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
    @overload
    def __rshift__(self, other: DataItem | int) -> DataItem: ...
    @overload
    def __rshift__(self, other: data_slice.DataSlice) -> data_slice.DataSlice: ...
